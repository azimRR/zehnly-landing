{
  "builds": [
    {
      "src": "zehnly_landing_page-main/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "contact.html",
      "use": "@vercel/static"
    },
    {
      "src": "privacy-policy.html",
      "use": "@vercel/static"
    },
    {
      "src": "promocode.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/contact", "dest": "/contact.html" },
    { "src": "/privacy-policy", "dest": "/privacy-policy.html" },
    { "src": "/promocode/(.*)", "dest": "/promocode.html" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/zehnly_landing_page-main/$1" }
  ]
}